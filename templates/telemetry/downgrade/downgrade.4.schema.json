{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "title": "downgrade",
  "properties": {
    @TELEMETRY_ID_1_JSON@,
    @TELEMETRY_CREATIONDATE_1_JSON@,
    @TELEMETRY_APPLICATION_1_JSON@,
    @TELEMETRY_CLIENTID_1_JSON@,
    "type": {
      "type": "string",
      "enum": [ "downgrade" ]
    },
    "version": {
      "type": "number",
      "minimum": 4,
      "maximum": 4
    },
    "payload": {
      "type": "object",
      "properties": {
        "lastVersion": {
          "type": "string",
          "pattern": "^[0-9]{2,3}\\."
        },
        "lastBuildId": {
          "type": "string",
          "pattern": "^[0-9]{10}"
        },
        "hasSync": {
          "type": "boolean"
        },
        "button": {
          "type": "number",
          "minimum": 0,
          "maximum": 2
        }
      },
      "required": [
        "lastVersion",
        "hasSync",
        "button"
      ]
    }
  },
  "required": [
    "type",
    "id",
    "creationDate",
    "version",
    "application",
    "clientId",
    "payload"
  ]
}

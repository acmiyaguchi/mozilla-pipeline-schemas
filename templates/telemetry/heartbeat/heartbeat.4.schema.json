{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "heartbeat",
  "type": "object",
  "properties": {
    @TELEMETRY_APPLICATION_1_JSON@,
    @TELEMETRY_CLIENTID_1_JSON@,
    @TELEMETRY_CREATIONDATE_1_JSON@,
    @TELEMETRY_ID_1_JSON@,
    "engagementType": {
      "enum": [
        "stars",
        "button1",
        "button2"
      ],
      "type": "string"
    },
    "payload": {
      "additionalProperties": false,
      "properties": {
        "version": {
          "maximum": 1,
          "minimum": 1,
          "type": "number"
        },
        "flowId": {
          @COMMON_PATTERN_UUID_1_JSON@,
          "type": "string"
        },
        "offeredTS": {
          "minimum": 0,
          "type": "integer"
        },
        "learnMoreTS": {
          "minimum": 0,
          "type": "integer"
        },
        "votedTS": {
          "minimum": 0,
          "type": "integer"
        },
        "engagedTS": {
          "minimum": 0,
          "type": "integer"
        },
        "closedTS": {
          "minimum": 0,
          "type": "integer"
        },
        "expiredTS": {
          "minimum": 0,
          "type": "integer"
        },
        "windowClosedTS": {
          "minimum": 0,
          "type": "integer"
        },
        "score": {
          "minimum": 1,
          "type": "integer"
        },
        "surveyId": {
          "type": "string"
        },
        "surveyVersion": {
          "pattern": "^([0-9]+|[a-fA-F0-9]{64})$",
          "type": "string"
        },
        "testing": {
          "type": "boolean"
        }
      },
      "required": [
        "version",
        "flowId",
        "offeredTS",
        "surveyId",
        "surveyVersion"
      ],
      "anyOf": [
        {
          "required": [
            "closedTS"
          ]
        },
        {
          "required": [
            "windowClosedTS"
          ]
        }
      ],
      "type": "object"
    },
    "type": {
      "enum": [
        "heartbeat"
      ],
      "type": "string"
    },
    "version": {
      "maximum": 4,
      "minimum": 4,
      "type": "number"
    }
  },
  "required": [
    "type",
    "id",
    "creationDate",
    "version",
    "clientId",
    "application",
    "payload"
  ]
}
